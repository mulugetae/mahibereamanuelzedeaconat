mixin Service(config)
  .col-sm-6.col-lg-3.mb-6.text-center.text-sm-start
    img.shadow-icon(src=`${CWD}assets/img/icons/${config.icon}` height='60' alt='...')
    h4.mt-4.fw-normal.text-200=config.title
    p.text-400.mb-md-0!=config.text

mixin Icons(config)
  .icon-box
    img.car-insurance-icon(src=`${CWD}assets/img/icons/${config.image}`   alt='...' )  
    img.car-insurance-icon.car-insurance-icon-hover(src=`${CWD}assets/img/icons/${config.hoverImage}`  alt='...' )

mixin TabContent(config)
  .col-md-6.col-xl-4.order-1.order-md-0
    h2.text-light.mb-md-5=config.title
    p.text-400!=config.textOne
    p.text-400.mt-3!=config.textTwo
    //- a.btn.btn-primary.btn-ensurance.my-md-5(href='#!')
    //-   span.fas.fa-chevron-right.fa-xs.btn-icon
    //-   span.btn-text Learn more  
  .col-md-6.col-xl-8.text-end
    .img-shape
      img.feature-img(src=`${CWD}assets/img/gallery/${config.image}` alt='...')

mixin CarInsurance
  section#products
    +Background({ image: `${CWD}assets/img/illustrations/bg-car-insurance.png`, attrs: { style: { 'background-position': '0 200px', 'background-size': '100% 45%'} }, className: 'd-none d-md-block' })
    +Background({ image: `${CWD}assets/img/illustrations/bg-left.png`, attrs: { style: { 'background-position': 'left', 'background-size': '15% 100%'} }, className: 'd-none d-md-block' })
    +Background({ image: `${CWD}assets/img/illustrations/circle.png`, attrs: { style: { 'background-position': '18% 49%;', 'background-size': 'auto'} }, className: 'd-none d-xl-block  mt-11' })
    .container-lg
      .row.flex-center.py-8.py-md-10
        .col-12.mt-6
          .d-flex.align-items-start
            .nav.flex-column.nav-tab#nav-tab(role='tablist' aria-orientation='vertical')
              a.circle-hover-wrapper.nav-link.mb-lg-4.active#nav-car-tab( href="#!" data-bs-toggle='tab' data-bs-target='#nav-car'  role='tab' aria-controls='nav-car' aria-selected='true') 
                +Icons(insurance[0])
                 
              a.circle-hover-wrapper.nav-link.mb-lg-4#nav-home-tab(  href="#!" data-bs-toggle='tab' data-bs-target='#nav-home'  role='tab' aria-controls='nav-home' aria-selected='false') 
                +Icons(insurance[1])

              a.circle-hover-wrapper.nav-link.mb-lg-4#nav-paw-tab(  href="#!" data-bs-toggle='tab' data-bs-target='#nav-paw'  role='tab' aria-controls='nav-paw' aria-selected='false') 
                +Icons(insurance[2])

              
                  
            .tab-content#nav-tabContent.w-100
              .tab-pane.fade.show.active#nav-car(role='tabpanel' aria-labelledby='nav-car-tab') 
                .row.align-items-center.offset-sm-1.g-3
                  +TabContent(content[0])
              .tab-pane.fade#nav-home(role='tabpanel' aria-labelledby='nav-home-tab') 
                .row.align-items-center.offset-sm-1.g-3
                  +TabContent(content[1])
              .tab-pane.fade#nav-paw(role='tabpanel' aria-labelledby='nav-paw-tab')
                .row.align-items-center.offset-sm-1.g-3 
                  +TabContent(content[2])
              
